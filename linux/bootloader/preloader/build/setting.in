###################################################################
# Symbol
###################################################################

D_BIN	= $(PRELOADER_OUT)/bin
D_OBJ	= $(PRELOADER_OUT)/obj
###################################################################
# CHIP , PLATFORM , TGTCPU and ENV definition
###################################################################

VERSION = 2
C_DEF	+= $(CHIP) BOOTROM_VERSION=$(VERSION)


PLATFORM = _BOARD
C_DEF	+= $(PLATFORM)

TGTCPU = $(_CPU)

ENV = preloader

C_DEF	+= $(ENV)

DEFINE   = $(foreach def,$(C_DEF),-D$(def))
